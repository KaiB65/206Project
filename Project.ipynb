import math as m
import numpy as np
import matplotlib.pyplot as plt



#G_eff solely relies on F_cent (then divided by m), so:
def F_cent_rpm(R,rpm,m):
    g_eff_array = []
    for i in range(len(rpm)):
        w = rpm[i]*((2*np.pi)/60)
        f_cent = m*(w**2)*R
        g_eff = (f_cent)/m
        g_eff_array.append(g_eff)
    return g_eff_array

R = 5.8 #Meters
m = 80 #Kilograms
rpm = np.arange(0,15,0.01) #0 to 15 rotations/minute with a step size of 0.1

g_eff_rpm = F_cent_rpm(R,rpm,m)

plt.plot(rpm,g_eff_rpm,'r-')
plt.hlines(y=9.81, xmin = 0, xmax=12.49) #Line showing intersection of g_eff = 9.81 (Earth Gravity), with the graph (rpm value necessary is approximately 12.42, but changed to 12.49 to better fit the graph)
plt.xlabel("Rotations Per Minute")
plt.ylabel("Effective Gravity (in terms of m/s^2)")
plt.title("Effective Gravity vs RPM with Radius=5.8m")
plt.grid()
