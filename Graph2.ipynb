#Graphing Effective Gravity (m/s^2) vs Radius (m) with rpm held constant at 3 (near uppper limit of human comfort threshold)

import math as m
import numpy as np
import matplotlib.pyplot as plt



#G_eff solely relies on F_cent (then divided by m), so:
def F_cent_R(R,rpm,m):
    g_eff_array = []
    for i in range(len(R)):
        w = rpm*((2*np.pi)/60)
        f_cent = m*(w**2)*R[i]
        g_eff = (f_cent)/m
        g_eff_array.append(g_eff)
    return g_eff_array

rpm = 3
m = 80 #Kilograms
R = np.arange(0,200,0.01) #0 to 15 rotations/minute with a step size of 0.1

g_eff_R = F_cent_R(R,rpm,m)

plt.plot(R,g_eff_R,'r-')
plt.hlines(y=9.81, xmin = 0, xmax=200) #Line showing intersection of g_eff = 9.81 (Earth Gravity), with the graph (R value necessary is approximately 99.4m, but changed to 100m to better fit the graph)
plt.xlabel("Radius (metres)")
plt.ylabel("Effective Gravity (in terms of m/s^2)")
plt.title("Effective Gravity vs Radius(m) at 3 rpm")
plt.grid()
